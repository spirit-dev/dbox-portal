{% if not project.gitCommitLastUpdate is empty %}

    {% set format = "Y-m-d" %}

    {# Setting formated dates #}
    {% set dateTen = "now"|date_modify("-10 day")|date(format) %}
    {% set dateSeven = "now"|date_modify("-7 day")|date(format) %}
    {% set dateThree = "now"|date_modify("-3 day")|date(format) %}
    {% set dateNow = "now"|date(format) %}
    {% set pjt_date = project.gitCommitLastUpdate|date(format) %}

    {# Analyzing scale #}
    {% if pjt_date >= dateTen and pjt_date < dateSeven %}
        <img class="tooltiped staring_1" data-toggle="tooltip" data-placement="left" title="Updated last week"
             src="{{ asset('bundles/spiritdevdboxportal/images/signs/Stared1.png') }}">
    {% elseif pjt_date >= dateSeven and pjt_date < dateThree %}
        <img class="tooltiped staring_1" data-toggle="tooltip" data-placement="left" title="Updated this week"
             src="{{ asset('bundles/spiritdevdboxportal/images/signs/Stared3.png') }}">
    {% elseif pjt_date >= dateThree and pjt_date < dateNow %}
        <img class="tooltiped staring_1" data-toggle="tooltip" data-placement="left" title="Updated last three days"
             src="{{ asset('bundles/spiritdevdboxportal/images/signs/Stared6.png') }}">
    {% elseif pjt_date == dateNow %}
        <img class="tooltiped staring_1" data-toggle="tooltip" data-placement="left" title="Updated today"
             src="{{ asset('bundles/spiritdevdboxportal/images/signs/Stared9.png') }}">
    {% endif %}

{% endif %}

{% if project.gitNbCommits > 0 %}

